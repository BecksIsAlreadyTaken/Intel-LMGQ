{
    "name": "nsfw block",
    "description": "init version",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "images/on.png",
        "default_title": "nsfw block"
    },
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "<all_urls>",
        "webRequest",
        "webRequestBlocking",
        "browsingData"
    ],
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "css": ["css/mycss.css"],
        "js": ["js/jquery.min.js", "js/content.js"]
    }],
    "background": {
        "scripts": ["js/jquery.min.js", "js/background.js"],
        "presistent": true
    },
    "web_accessible_resources": [
        "images/*"
    ],
    "options_page": "html/options.html",
    "content_security_policy": "script-src 'self' ; object-src 'self'"
}